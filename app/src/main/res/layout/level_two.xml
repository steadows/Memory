<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#abcdef"
    tools:context=".LevelTwo" >

    <View
        android:id="@+id/game_board_viewM"
        android:layout_width="395dp"
        android:layout_height="577dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="#74264E91"
        app:layout_constraintBottom_toTopOf="@id/scorecard_viewM"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/scorecard_viewM"
        android:layout_width="0dp"
        android:layout_height="118dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="#74264E91"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/matches_textM"
        android:layout_width="180dp"
        android:layout_height="32dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="18dp"
        android:fontFamily="sans-serif-medium"
        android:text="@string/matches"
        android:textAlignment="textEnd"
        android:textAllCaps="true"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        android:textColor="#E6BC3F"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/scorecard_viewM"
        app:layout_constraintTop_toTopOf="@+id/scorecard_viewM"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/time_elapsed_textM"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="70dp"
        android:fontFamily="sans-serif-medium"
        android:text="@string/time_elapsed"
        android:textAlignment="textEnd"
        android:textAllCaps="true"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        android:textColor="#E6BC3F"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/scorecard_viewM"
        app:layout_constraintTop_toTopOf="@+id/scorecard_viewM"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/timer_textM"
        android:layout_width="120dp"
        android:layout_height="45dp"
        android:layout_marginStart="220dp"
        android:layout_marginTop="62dp"
        android:fontFamily="sans-serif-black"
        android:text="@string/_0_00_00"
        android:textAllCaps="false"
        android:textColor="#FF0000"
        android:textSize="34sp"
        android:typeface="monospace"
        app:layout_constraintStart_toStartOf="@+id/scorecard_viewM"
        app:layout_constraintTop_toTopOf="@+id/scorecard_viewM"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/score_number_textM"
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="220dp"
        android:layout_marginTop="12dp"
        android:fontFamily="sans-serif-black"
        android:text="@string/_0"
        android:textColor="#FF0000"
        android:textSize="34sp"
        android:typeface="monospace"
        app:layout_constraintStart_toStartOf="@+id/scorecard_viewM"
        app:layout_constraintTop_toTopOf="@+id/scorecard_viewM"
        tools:ignore="TextContrastCheck" />

    <com.gusakov.library.PulseCountDown
        android:id="@+id/pulseCountDownM"
        android:layout_width="245dp"
        android:layout_height="454dp"
        android:drawableTint="#000000"
        android:text="@string/_5"
        android:textColor="#9F000000"
        android:textSize="400sp"
        android:translationZ="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.084"
        app:pc_startValue="5"
        tools:ignore="TextContrastCheck" />

